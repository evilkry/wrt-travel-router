
config wifi-device 'radio0'
        option type 'mac80211'
        option channel '36'
        option hwmode '11a'
        option path 'platform/soc/c000000.wifi'
        option htmode 'VHT80'
        option country 'US'
        option legacy_rates '0'
        option random_bssid '1'

config wifi-iface 'wifinet0'
        option device 'radio0'
        option network 'vlan4'
        option mode 'ap'
        option ssid 'ProductionMobile'
        option encryption 'psk2'
        option key 'Redacted_Password'
        option ieee80211k '1'
        option ieee80211v '1'
        option ieee80211r '1'
        option ifname 'wlan0'
        option macaddr 'FA:79:2F:92:16:AF'

config wifi-device 'radio1'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/soc/c000000.wifi+1'
        option htmode 'HT20'
        option country 'US'
        option legacy_rates '0'
        option random_bssid '1'

config wifi-iface 'wifinet1'
        option device 'radio1'
        option network 'vlan4'
        option mode 'ap'
        option ssid 'ProductionMobile'
        option encryption 'psk2'
        option key 'Redacted_Password'
        option ieee80211k '1'
        option ieee80211r '1'
        option ifname 'wlan1'
        option macaddr '0E:30:A3:AF:B0:60'
